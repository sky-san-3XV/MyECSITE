<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
<package name="com.internousdev.ecsitestudy.action" extends="struts-default">

<action name="HomeLoginAction" class="com.internousdev.ecsitestudy.action.HomeLoginAction"
method="execute">
<result name="success">login.jsp</result>
<result name="error">systemError.html</result>
</action>

<action name="LoginAction" class="com.internousdev.ecsitestudy.action.LoginAction"
method="execute">
<result name="success">home.jsp</result>
<result name="CART">cart.jsp</result>
<result name="error">login.jsp</result>
</action>

<action name="PasswordResetAction" class="com.internousdev.ecsitestudy.action.PasswordResetAction"
method="execute">
<result name="success">passwordreset.jsp</result>
</action>

<action name="PasswordResetConfirmAction" class="com.internousdev.ecsitestudy.action.PasswordResetConfirmAction"
method="execute">
<result name="success">passwordresetConfirm.jsp</result>
<result name="error">passwordreset.jsp</result>
</action>
<action name="PasswordResetCompleteAction" class="com.internousdev.ecsitestudy.action.PasswordResetCompleteAction"
method="execute">
<result name="success">passwordresetComplete.jsp</result>
<result name="ERROR">systemError.html</result>
</action>

<action name="CreateUserAction" class="com.internousdev.ecsitestudy.action.CreateUserAction"
method="execute">
<result name="success">CreateUser.jsp</result>


</action>
<action name="AddCartAction" class="com.internousdev.ecsitestudy.action.AddCartAction"
method="execute">
<result name="success">cart.jsp</result>
<result name="LOGIN">login.jsp</result>
</action>

<action name="CartAction" class="com.internousdev.ecsitestudy.action.CartAction"
method="execute">
<result name="success">cart.jsp</result>
<result name="LOGIN">login.jsp</result>
</action>

<action name="DeleteCartAction" class="com.internousdev.ecsitestudy.action.DeleteCartAction"
method="execute">
<result name="success">cart.jsp</result>
</action>
<action name="SettlementConfirmAction" class="com.internousdev.ecsitestudy.action.SettlementConfirmAction"
method="execute">
<result name="success">settlementconfirm.jsp</result>
<result name="login">login.jsp</result>
</action>

<action name="SettlementCompleteAction" class="com.internousdev.ecsitestudy.action.SettlementCompleteAction"
method="execute">
<result name="success">settlementcomplete.jsp</result>
<result name="error">systemError.html</result>
</action>

<action name="CreateDestinationAction" class="com.internousdev.ecsitestudy.action.CreateDestinationAction"
method="execute">
<result name="success">CreateDestination.jsp</result>
</action>

<action name="CreateDestinationConfirmAction" class="com.internousdev.ecsitestudy.action.CreateDestinationConfirmAction"
method="execute">
<result name="success">CreateDestinationConfirm.jsp</result>
<result name="error">CreateDestination.jsp</result>
</action>

<action name="CreateDestinationCompleteAction" class="com.internousdev.ecsitestudy.action.CreateDestinationCompleteAction"
method="execute">
<result name="success">CreateDestinationComplete.jsp</result>
<result name="error">systemError.html</result>
</action>

<action name="DeleteDestinationAction" class="com.internousdev.ecsitestudy.action.DeleteDestinationAction"
method="execute">
<result name="success">settlementconfirm.jsp</result>
</action>

<action name="UserCreateConfirmAction" class="com.internousdev.ecsitestudy.action.UserCreateConfirmAction"
method="execute">
<result name="success">CreateUserConfirm.jsp</result>
<result name="error">CreateUser.jsp</result>
</action>
<action name="UserCreateCompleteAction" class="com.internousdev.ecsitestudy.action.UserCreateCompleteAction"
method="execute">
<result name="success">UserCreateComplete.jsp</result>
<result name="ERROR">systemError.html</result>
</action>

<action name="ProductsAction" class="com.internousdev.ecsitestudy.action.ProductsAction"
method="execute">
<result name="success">products.jsp</result>
</action>

<action name="ProductsDetailAction" class="com.internousdev.ecsitestudy.action.ProductsDetailAction"
method="execute">
<result name="success">productinfo.jsp</result>
</action>

<action name="HomeAction" class="com.internousdev.ecsitestudy.action.HomeAction"
method="execute">
<result name="success">home.jsp</result>
</action>

<action name="MyPageAction" class="com.internousdev.ecsitestudy.action.MyPageAction"
method="execute">
<result name="success">MyPage.jsp</result>
</action>

<action name="PurchaseHistoryAction" class="com.internousdev.ecsitestudy.action.PurchaseHistoryAction"
method="execute">
<result name="success">purchasehistory.jsp</result>
</action>


<action name="DeletePurchaseHistoryAction" class="com.internousdev.ecsitestudy.action.DeletePurchaseHistoryAction"
method="execute">
<result name="success">purchasehistory.jsp</result>
</action>

<action name="LogoutAction" class="com.internousdev.ecsitestudy.action.LogoutAction"
method="execute">
<result name="success">home.jsp</result>
</action>

<action name="SearchAction" class="com.internousdev.ecsitestudy.action.SearchAction"
method="execute">
